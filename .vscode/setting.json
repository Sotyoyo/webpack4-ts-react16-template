    "prettier.semi": false,
    "prettier.singleQuote": true,
    "prettier.useTabs": false,
    "prettier.tabWidth": 4,
    "prettier.trailingComma": "es5",
    "prettier.printWidth": 120,
    "editor.formatOnSave": true,
    "editor.wordWrap": "on",
    "editor.wordWrapColumn": 120,
    "eslint.enable": true,
